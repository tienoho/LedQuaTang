    <style>
        .asterisk {
            font-weight: normal;
            color: red;
        }

        .action {
            margin-left: 5px;
        }

        .error {
            font-weight: normal;
        }

        ::-webkit-input-placeholder {
            font-style: italic;
        }

        .sp {
            color: red;
        }

        :-moz-placeholder {
            font-style: italic;
        }

        ::-moz-placeholder {
            font-style: italic;
        }

        :-ms-input-placeholder {
            font-style: italic;
        }

        .form-inline {
            margin-bottom: 5px
        }

        #searchclear {
            position: absolute;
            right: 15px;
